  
x = 0

def setup():
    size(1000, 1000)
    background(255)
    textAlign(CENTER)
    
def draw():
   print("")
    
def mouseClicked():
    x = mouseX
    y = mouseY
    r = random(10,100)
    myred = random(0,255)
    myred = random(0, 255)
    myblue = random(0,255)
    mygreen = random(0,255)
    fill(myred, mygreen, myblue)
    
    line(500, 500, mouseX, mouseY)
    circle(x, y, r)
    fill(255)
    text("m", x,y)
    
    print (x, y)
    
